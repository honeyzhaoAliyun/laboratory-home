{
  "required": true,
  "$schema": "http://json-schema.org/draft-extends/schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "required": false
    },
    "name": {
      "type": "string",
      "required": false
    },
    "status": {
      "type": "string",
      "required": false
    },
    "subject": {
      "$ref": "subject"
    },
    "maxQuantity": {
      "type": "string",
      "required": false
    },
    "minQuantity": {
      "type": "string",
      "required": false
    },
    "ageRange": {"$ref": "ageRange"},
    "topicSections": {
      "type": "array",
      "items": {
        "$ref": "topicSection"
      }
    },
    "city": {"$ref":"city"}
  }
}
