{
  "required": true,
  "$schema": "http://json-schema.org/draft-extends/schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "required": false
    },
    "title": {
      "type": "string",
      "required": false
    },
    "image": {
      "type": "string",
      "required": false
    },
    "desc": {
      "type": "string",
      "reuqired": false
    },
    "status": {
      "type": "string",
      "required": false
    },
    "publisher": {
      "type": "object",
      "$ref": "publisher"
    },
    "tags": {
      "type": "string",
      "required": false
    },
    "funs": {
      "type": "array",
      "items": {
        "$ref": "funs"
      }
    },
    "posters": {
      "type": "array",
      "items": {
        "$ref": "poster"
      }
    }
  }
}
