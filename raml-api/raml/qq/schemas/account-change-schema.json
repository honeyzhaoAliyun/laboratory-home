    "frozenAmount": {
      "description": "冻结金额",
      "type": "number",
      "required": false
    },
    "prelicensingAmount": {
      "description": "预授权金额",
      "type": "number",
      "required": false
    },
    "amount": {
      "description": "可用金额",
      "type": "number",
      "required": false
    },
    "memberId": {
      "description": "会员ID",
      "type": "string",
      "required": false
    },
    "changesTime": {
      "description": "变更时间",
      "type": "string",
      "required": false
